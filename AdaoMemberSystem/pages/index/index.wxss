.mainTitle{
  width:100%;
  text-align: center;
  font-size: 1.2rem;
}

.title{
  display: block;
  align-items: center;
  justify-content: center;
}
.TitleIcons{
  width: 100%;
  height: 50px;
  padding-top: 5px;
  margin: 0 auto;
}
.loginTitle2{
  color: #53d192;
  font-weight: bold;
  font-size: 40px;
  text-align: center;
}
.navigate{
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
}
.minMenu{
  font-size: 15px;
  text-align: center;
  padding-top: 30px;
  min-width: 33%;
}
.menuSelected{
  color: #53d192;
}

.lf{
  width: 100%;
  opacity: 0;
}

.LoginBut{
 width: 80%;
 margin-top: 10px;
}
.RememberPW{
  margin: 0 auto;
  border-top: 1px solid #A9A9A9;
  border-bottom: 1px solid #A9A9A9;
  margin-top: 5px;
  width: 80%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.RememberSW{
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 10px;
}
.AppDownload{
  text-align: center;
  width: 100%;
  color: #A9A9A9;
  margin-top: 15px;
  margin-bottom: 50px;
}
.user-agreement{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-size: 1rem;
  margin-top: 0.5rem;
  margin-bottom: 0.5rem;
}
.hight-light{
  color:#5eb95e;
  text-decoration: underline;
}
.modal{
  width: 80%;
  height: 80%;
  position: fixed;
  margin: 0 auto;
  background-color: #ffe;
  border: 1px solid #fff;
  border-radius: 5px;
  left: 10%;
  top: 5%;
  box-shadow: 2px 2px 20px #FFF;
  overflow-y: scroll;
  overflow-x: hidden;
  z-index: 999;
}
.small-image{
  height: 1rem;
  width: 1rem;
}
.modal-close{
  width: 100%;
  background-color: #ffe;
  color:#53d192;
  border-top: 1px solid #000;
}